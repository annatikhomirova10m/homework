// Максимальный простой делитель

#include "pch.h"
#include <iostream>
#include <string>

using namespace std;
int maxSimple(int a) {
	int Result = 1;
	int vala = a;
	while (vala > 1) {
		for (int i = 2; i <= vala; i++) {
			if (vala % i == 0) {
				Result = i;
				vala = vala / i;
				break;
			}
		}
	}
	return Result;
}
